#  - Regex: '^.*(precomp|pch|stdafx)'
#    Priority: -1
#  - Regex: '^".*"'
#    Priority: 1
#  - Regex: '^<.*>'
#    Priority: 2
#  - Regex: '.*'
#    Priority: 3